{
	"name" : "kaikmedia/pages-module",
	"version" : "2.0.0",        
	"description" : "Module to manage simple html pages",
	"type" : "zikula-module",
	"license" : "LGPL-3.0+",
	"authors" : [{
			"name" : "Kaik",
			"homepage" : "http://kaikmedia.com/",
			"email" : "contact@kaikmedia.com"
		}
	],
	"autoload" : {
		"psr-4" : {
			"Kaikmedia\\PagesModule\\" : ""
		}
	},
	"require" : {
		"php" : ">5.3.3"
	},
	"extra" : {
		"zikula" : {
			"core-compatibility" : ">=1.4.1",                   
			"class" : "Kaikmedia\\PagesModule\\KaikmediaPagesModule",
			"displayname" : "Pages",
			"url" : "pages",
			"oldnames" : [
				"Pages"
			],
			"capabilities" : {
                                "hook_subscriber": {"class": "Kaikmedia\\PagesModule\\Container\\HookContainer"},
                                "searchable": {"class": "Kaikmedia\\PagesModule\\Helper\\SearchHelper"},
                                "categorizable": ["Kaikmedia\\PagesModule\\Entity\\PageEntity"],                            
				"user" : { "route" : "kaikmediapagesmodule_user_index"},
				"admin" : {"route" : "kaikmediapagesmodule_admin_index"}
			},
			"securityschema" : {
				"KaikmediaPagesModule::" : "::"
			}                       
		}
	}
}